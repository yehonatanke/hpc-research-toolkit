{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "readable": {
            "format": "%(asctime)s | %(levelname)-7s | %(name)s:%(funcName)s:%(lineno)d - %(message)s",
            "datefmt": "%d-%m-%Y %H:%M:%S"
        },
        "readable_file": {}, ///edit this to like: python -m scripts.run_depth_overlay --rgb_path /leonardo_work/AIFAC_S02_060/data/yk/debug/dl3dv_wai_dummy/1K_0a1b7c20a92c43c6b8954b1ac909fb2f0fa8b2997b80604bc8bbec80a1cb2da3/images/frame_00001.jpg --depth_path /leonardo_work/AIFAC_S02_060/data/yk/debug/dl3dv_wai_dummy/1K_0a1b7c20a92c43c6b8954b1ac909fb2f0fa8b2997b80604bc8bbec80a1cb2da3/mvsanywhere/v0/depth/frame_00001.exr
        "readable_colored": {
            "format": "%(asctime)s | %(levelname)-7s | %(name)s:%(funcName)s:%(lineno)d - %(message)s",
            "datefmt": "%d-%m-%Y %H:%M:%S"
        },
        "json": {
            "()": "research_utils.logging.handlers.JSONFormatter",
            "fmt_keys": {
                "level": "levelname",
                "logger": "name",
                "line": "lineno"
            }
        },
        "colored": {
            "()": "research_utils.logging.handlers.ColoredFormatter"
        },
        "readable_color_stdout": {
            "()": "research_utils.logging.handlers.ReadableColorFormatter"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "INFO",
            "formatter": "readable",
            "filename": "logs/app.log",
            "maxBytes": 10485760,
            "backupCount": 5
        },
        "stdout": {
            "class": "logging.StreamHandler",
            "formatter": "readable_color_stdout",
            "level": "DEBUG", ///change this
            "stream": "ext://sys.stdout"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": [
            "stdout",
            "file"
        ]
    }
}